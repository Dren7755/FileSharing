@model IEnumerable<FileSharing.Models.FileModel.File>
@{
    ViewData["Title"] = "List of users";
}

<h2>Ваши файлы</h2>

<table class="table">
    <tr>
        <td>Имя файла</td><td>Пароль</td><td>Тип</td><td>Дата загрузки</td>
    </tr>
    @foreach (var file in Model)
    {
        <tr>
            <td>
                <a href="@file.Link.Url">@file.FileName</a>
            </td>
            <td>@file.Link.AccessPassword</td>
            <td>@file.ContentType</td>
            <td>@file.CreatedDate</td>
        </tr>
    }
</table>