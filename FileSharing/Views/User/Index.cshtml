@model IEnumerable<FileSharing.Models.UserModel.User>
@{
    ViewData["Title"] = "List of users";
}

<a asp-action="registration">Регистрация</a>

<table class="table">
    <tr><td>Email</td><td>Login</td><td>Password</td></tr>
    @foreach (var item in Model)
    {
        <tr><td>@item.Email</td><td>@item.Login</td><td>@item.Password</td></tr>
    }
</table>